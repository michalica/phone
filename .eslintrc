{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "env": {
    "jest": true
  },
  "rules": {
    "no-console": 1,
    "@typescript-eslint/no-var-requires": 0
  },
  "overrides": [
    {
      "files": ["src/**/*.test.ts"],
      "rules": {
        "no-console": "off"
      }
    }
  ]
}